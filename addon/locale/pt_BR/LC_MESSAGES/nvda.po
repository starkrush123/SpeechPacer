#: addon\globalPlugins\pausingInfo.py:148
msgid "Message Extension"
msgstr "Extensão de mensagens"

#. Translators: The label for a radio button option in the settings panel.
#: addon\globalPlugins\pausingInfo.py:152
msgid "&Short"
msgstr "&Curtas"

#. Translators: The label for a radio button option in the settings panel.
#: addon\globalPlugins\pausingInfo.py:156
msgid "&Medium"
msgstr "&Médias"

#. Translators: The label for a radio button option in the settings panel.
#: addon\globalPlugins\pausingInfo.py:160
msgid "&Long"
