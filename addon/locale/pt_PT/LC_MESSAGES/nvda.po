#: addon\globalPlugins\pausingInfo.py:161
msgid "&Short"
msgstr "&Curta"

#. Translators: The label for a radio button option in the settings panel.
#: addon\globalPlugins\pausingInfo.py:165
msgid "&Medium"
msgstr "&Média"

#. Translators: The label for a radio button option in the settings panel.
#: addon\globalPlugins\pausingInfo.py:169
msgid "&Long"
msgstr "&Longa"

#. Translators: The label for a radio button option in the settings panel.
#: addon\globalPlugins\pausingInfo.py:173
msgid "&Custom"
